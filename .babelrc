{
  "presets": [
    ["next/babel"]
  ],
    "plugins": [
        [
            "styled-components",
            {
                "ssr":true,
                "displayName":true,
                "preprocess":false,
                "fileName":true,
                "pure":true
            }
        ],
        ["@babel/plugin-proposal-decorators",{"legacy":true}],
        ["@babel/plugin-proposal-class-properties",{"loose":true}]
    ]
}